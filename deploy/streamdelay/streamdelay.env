# /etc/streamdelay/streamdelay.env

# API
API_HOSTNAME=0.0.0.0
API_PORT=7070
API_KEY=lan-key

# SRT input (listener)
SRT_IN_URI=srt://0.0.0.0:9000?mode=listener&latency=200000

# Core behavior
DELAY_SECONDS=15
RESTART_SECONDS=3

# Overlay image required by streamdelay
OVERLAY_IMG=/etc/streamdelay/overlay.png

# Start pipeline on service start
START_ON_BOOT=false
